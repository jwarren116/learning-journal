{% extends "base.jinja2" %}

{% block body %}

<article id="title={{title.id}}">
  <h1 class="headingLink">{{ title.title }}</h1>
  <p><small>{{ entry.created.strftime('%b. %d, %Y') }}</small>
  <div>
    {{ title.text|safe }}
    </br></br>
    <hr class="titleDivider">
  </div>
</article>

{% endblock %}